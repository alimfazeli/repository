{
    "name": "alimfazeli/repositories",
    "description": "Laravel Repositories",
    "license": "MIT",
    "homepage": "https://alimfazeli.com",
    "keywords": [ "laravel", "repository", "repositories", "eloquent", "database" ],
    "authors": [ {
        "name": "Ali Fazeli",
        "email": "alimfazeli@gmail.com"
    } ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^5.1",
        "illuminate/database": "^5.1",
        "illuminate/filesystem": "^5.1",
        "illuminate/console": "^5.1"
    },
    "require-dev": {
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "psr-4": {
            "Bosnadev\\": "src/Alimfazeli/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bosnadev\\Repositories\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
